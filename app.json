{
  "expo": {
    "name": "Doggy",
    "slug": "Doggy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/doggy_logo_icon.png",
    "splash": {
      "image": "./assets/doggy_logo_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#F8F9FA"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["./assets/fonts/*.ttf", "./assets/*.png"],
    "android": {
      "package": "com.LL.Doggy",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#F8F9FA"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
